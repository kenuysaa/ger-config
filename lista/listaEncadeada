class Node:
    def __init__(self, data):
        self.data = data  # valor armazenado
        self.next = None  # referência para o próximo nó

class LinkedList:
    def __init__(self):
        self.head = None  # início da lista

    # Inserir no final
    def append(self, data):
        new_node = Node(data)
        if not self.head:  # se a lista estiver vazia
            self.head = new_node
            return
        current = self.head
        while current.next:  # percorre até o último
            current = current.next
        current.next = new_node

    # Mostrar a lista
    def display(self):
        current = self.head
        while current:
            print(current.data, end=" -> ")
            current = current.next
        print("None")


